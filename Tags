   -
   -git config --global alias.st status
   -git restore --staged
   -git log -n3
   -git config -l
   -git remote -v
   -git config --global --unset user.name
   -git config --global --edit
   -git stash
   -git stash show
   -git stash pop
   -git stash pop stash@{3}
   -git stash list
   -git stash drop stash@{1}
   -git clean -n
   -git clean -f
   -git reset --hard
   -git tag


